{
  "name": "react-resume-template",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "yarn compile && next build",
    "start": "next start",
    "clean": "rm -rf build-tsc .next",
    "compile": "tsc --noEmit",
    "lint": "yarn run -T prettier --write './src/**/*.{js,jsx,json,ts,tsx}' && eslint './src/**/*.{js,jsx,ts,tsx}' --fix --max-warnings=0",
    "sitemap": "yarn next-sitemap",
    "prettier": "prettier --write .",
    "format": "eslint './src/**/*.{js,jsx,ts,tsx}' --fix && prettier --write .",
    "tsc": "tsc --noEmit",
    "assets:pdf-thumbs": "powershell -NoProfile -ExecutionPolicy Bypass -File scripts/pdf-build-thumbs.ps1 -Root public -PreviewWidth 1600 -ThumbWidth 300"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@headlessui/react": "^1.7.16",
    "@heroicons/react": "^2.0.18",
    "autoprefixer": "10.4.16",
    "classnames": "^2.3.2",
    "next": "15",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.4.0",
    "ts-pattern": "^5.0.4"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "15.1.6",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/forms": "^0.5.4",
    "@tailwindcss/typography": "^0.5.9",
    "@types/node": "^22.10.7",
    "@types/prop-types": "^15.7.14",
    "@types/react": "19.0.8",
    "@types/semver": "^7.5.8",
    "@types/tailwindcss": "^3.1.0",
    "@types/webpack-env": "^1.18.5",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "cssnano": "^6.0.1",
    "eslint": "^8.46.0",
    "eslint-config-next": "15.1.6",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-module-resolver": "^1.5.0",
    "eslint-plugin-monorepo": "^0.3.2",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-memo": "^0.0.3",
    "eslint-plugin-react-native": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "postcss": "^8.4.27",
    "postcss-preset-env": "^9.1.0",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "sass": "^1.64.2",
    "sort-package-json": "^2.5.1",
    "stylelint": "^15.10.2",
    "stylelint-config-recommended": "^13.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-prettier": "^4.0.2",
    "tailwindcss": "^3.3.3",
    "typescript": "5.6.2"
